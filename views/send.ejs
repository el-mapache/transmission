<!DOCTYPE html>
<html lang="en">
<% include _head.ejs %>
<body>
  <div id="absolute-wrapper">
    <% include _header.ejs %>
    <div class="clearfix"></div>
    <div class="row grid-spacing">
      <div id='stats' class="span12">
        <div id="subscribers" class="span2 tx-hud center sm-pad">
          clients subscribing: <span class='hi-lite center grid-spacing'>0</span> 
        </div>
        <div id="mode" class="span2 tx-hud center sm-pad">
          mode: <span class='hi-lite center grid-spacing'>-</span> 
        </div>
        <div class='clearfix'></div>
      </div>
    </div>

    <div class="row">
      <div id="tx-container" class="span8 grid-spacing">
        <div id="tx-useage">
          <h3 class='center'>drag a file into the box below to transmit.</h3>
          <div class='clearfix'></div>
        </div>
        <div id="tx-drop-box" class="span8"></div>
      </div>
    </div>
  </div>
  <% include footer.ejs %>
</body>
</html>
